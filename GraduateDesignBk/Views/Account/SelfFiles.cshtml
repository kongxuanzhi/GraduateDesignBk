@model IEnumerable<GraduateDesignBk.Models.File>

@{
    ViewBag.Title = "SelfFiles";
    ViewDataDictionary dic = new ViewDataDictionary();
    dic.Add("Id", Model.Id);
    dic.Add("userType", Model.userType);
    dic.Add("self","文件");
}

@Html.Partial("_NavSelfPartial",dic)

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.User.Photo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FileSeq)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Public)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UploadTimes)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.User.Photo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FileSeq)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Public)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UploadTimes)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FID }) |
            @Html.ActionLink("Details", "Details", new { id=item.FID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FID })
        </td>
    </tr>
}

</table>
